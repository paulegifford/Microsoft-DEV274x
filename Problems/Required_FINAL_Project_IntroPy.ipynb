def get_names():
    names = []
    count = 1
    message = ""
    while count < 6 :
        message = input("enter the name of an element : ")
        if message == "":
            pass
        elif message in names:
            print(message,"was already entered <-- no duplicates allowed")
        else:
            names.append(message)
            count = count + 1
    return names
